{
    "name": "tiamat",
    "displayName": "Tiamat",
    "description": "",
    "version": "0.0.1",
    "publisher": "RESHAPELab",
    "engines": {
        "vscode": "^1.93.0"
    },
    "categories": [
        "AI",
        "Chat"
    ],
    "activationEvents": [
        "onChatParticipant:tiamat.Tiamat"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "chatParticipants": [
            {
                "id": "tiamat.Tiamat",
                "fullName": "Tiamat",
                "name": "tiamat",
                "description": "Ask me anything...",
                "isSticky": true,
                "commands": []
            }
        ],
        "authentication": []
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "node -r dotenv/config && tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@eslint/js": "^9.13.0",
        "@stylistic/eslint-plugin": "^2.9.0",
        "@types/mocha": "^10.0.3",
        "@types/node": "18.x",
        "@types/vscode": "^1.93.0",
        "@vscode/test-electron": "^2.3.6",
        "eslint": "^9.13.0",
        "glob": "^10.3.10",
        "mocha": "^10.2.0",
        "typescript": "^5.2.2",
        "typescript-eslint": "^8.16.0"
    },
    "dependencies": {
        "axios": "^1.7.9",
        "dotenv": "^16.4.7",
        "tiamat": "file:"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/RESHAPELab/tiamat-vscode"
    }
